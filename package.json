{
  "name": "ml-analytics",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package": "npm run -s package-python && npm run -s package-build && npm run -s package-cleanup",
    "package-build": "electron-builder --win",
    "package-python": "pyinstaller -w -F --add-data frontend;frontend --add-data modules;modules app.py --distpath dist-python",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Music Player\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64  --prune=true --out=release-builds",
    "package-cleanup": "rimraf build && rimraf app.spec && rimraf dist-python"
  },
  "author": "ejson03",
  "license": "ISC",
  "dependencies": {
    "ps-tree": "^1.2.0"
  },
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "rimraf": "^3.0.2"
  }
}
